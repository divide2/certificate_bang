<!--pages/login/login.wxml-->
<view class="padding">
  <form bindsubmit="login">
    <view class="cu-form-group">
      <view class="title">账号</view>
      <input placeholder="请输入机构名称" name="username"/>
    </view>
    <view class="cu-form-group">
      <view class="title">密码</view>
      <input placeholder="请输入长度为八位的密码" type="password" name="password"/>
    </view>
    <button class="cu-btn block bg-blue margin-top lg" form-type="submit">登录</button>
  </form>
</view>
